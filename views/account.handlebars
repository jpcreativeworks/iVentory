<main>
      <div class="box1">
            <div class="box4">
                <div class="sqaure2">
                    <h1>iVentory</h1>
                    <div class="form-group">
                        <p class="text-end"><label for="email-signup">Search our Database by Item:</label>
                            <input class="form-input" type="text" />
                            <button>Search</button>
                        </p>
                    </div>
                </div>
            </div>
        </div>
        <div class="box1 row">
            <div class="box5 container">
                <div class="sqaure3 col-6">
                    <form class="box3">
